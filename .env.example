# Environment Configuration
# Copy this to .env and configure for your environment

# Server Configuration
HOST=0.0.0.0
PORT=8000
RELOAD=false
LOG_LEVEL=info

# Cache Configuration
CACHE_TTL=3600

# Scraping Configuration
MAX_WORKERS=5
DOWNLOAD_TIMEOUT=40

# CORS Configuration (comma-separated list)
CORS_ORIGINS=*

# Temp Directory
TEMP_DIR=temp_downloads
